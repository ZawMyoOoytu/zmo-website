{
  "name": "zmo-admin-panel",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://zmo-admin.vercel.app",
  "description": "ZMO Website Admin Panel",
  "dependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "axios": "^1.6.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-grid-layout": "^1.5.2",
    "react-quill": "^2.0.0",
    "react-resizable": "^3.0.5",
    "react-router-dom": "^6.20.1",
    "react-scripts": "5.0.1",
    "react-toastify": "^10.0.4",
    "web-vitals": "^3.5.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "cp .env.local .env && react-scripts start",
    "dev:cors": "cp .env.local .env && cross-env HOST=0.0.0.0 BROWSER=none react-scripts start",
    "build:prod": "cp .env.production .env && react-scripts build",
    "build:local": "cp .env.local .env && react-scripts build",
    "env:local": "cp .env.local .env",
    "env:prod": "cp .env.production .env",
    "env:dev": "cp .env.development .env",
    "preview": "serve -s build",
    "predeploy": "npm run build:prod",
    "deploy": "vercel --prod",
    "deploy:preview": "vercel",
    "lint": "eslint src --ext .js,.jsx",
    "lint:fix": "eslint src --ext .js,.jsx --fix",
    "clean": "rm -rf build node_modules/.cache",
    "clean:all": "rm -rf build node_modules package-lock.json",
    "reset": "npm run clean:all && npm install",
    "doctor": "npx @react-workspaces/doctor",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "security": "npm audit",
    "security:fix": "npm audit fix"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^7.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^3.1.1",
    "serve": "^14.2.1",
    "source-map-explorer": "^2.5.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "resolutions": {
    "webpack": "^5.89.0"
  }
}