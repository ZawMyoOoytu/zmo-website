{
  "version": 2,
  "name": "zmo-admin-panel",
  "buildCommand": "DISABLE_ESLINT_PLUGIN=true npm run build",
  "outputDirectory": "build",
  "installCommand": "npm install --legacy-peer-deps",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "REACT_APP_API_URL": "https://zmo-backend.vercel.app/api",
    "REACT_APP_ADMIN_URL": "https://zmo-admin.vercel.app",
    "REACT_APP_FRONTEND_URL": "https://zmo-frontend.vercel.app",
    "REACT_APP_ENV": "production"
  }
}