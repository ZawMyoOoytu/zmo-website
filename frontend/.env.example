# ==========================================
# üöÄ ZMO FRONTEND - ENVIRONMENT TEMPLATE
# ==========================================
# COPY THIS FILE TO .env AND UPDATE VALUES

# üåê API CONFIGURATION
REACT_APP_API_URL=https://your-backend.onrender.com
REACT_APP_API_BASE_URL=https://your-backend.onrender.com/api
REACT_APP_API_BACKUP_URL=https://backup-backend.onrender.com
REACT_APP_API_TIMEOUT=10000

# üåç APPLICATION URLs
REACT_APP_ADMIN_URL=https://your-admin.vercel.app
REACT_APP_FRONTEND_URL=https://your-frontend.vercel.app
REACT_APP_WEBSITE_URL=https://your-website.vercel.app

# üîß ENVIRONMENT
REACT_APP_ENV=production
REACT_APP_VERSION=1.0.0
REACT_APP_DEPLOYMENT_PLATFORM=vercel
REACT_APP_BACKEND_PLATFORM=render

# üîê AUTHENTICATION
REACT_APP_TOKEN_REFRESH_INTERVAL=5400000
REACT_APP_SESSION_TIMEOUT=3600000

# üìä FEATURE FLAGS
REACT_APP_ANALYTICS_ENABLED=true
REACT_APP_DEBUG_MODE=false
REACT_APP_MAINTENANCE_MODE=false

# üîó EXTERNAL SERVICES
REACT_APP_GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX
REACT_APP_SUPPORT_EMAIL=support@yourdomain.com
REACT_APP_CONTACT_EMAIL=contact@yourdomain.com